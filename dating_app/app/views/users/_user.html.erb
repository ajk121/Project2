<div class="search_image" id ='user_<%= user.id %>' data-user-id="<%=user.id%>">
  <% if user.image.present? %>
    <%= link_to image_tag(user.image), user_path(user) %>
  <% else %>
    <%= link_to image_tag("male.jpg"), user_path(user) %>
  <% end %>
  <div class="user-info">
    <h3><%= link_to user.name, user_path(user) %></h3>
  </div>
</div>