<p id="notice"><%= notice %>

  <div class='profile_top_wrapper'>

    <div class='profile_image'>  
      <%= image_tag @user.image.url %>
      <h3><%= @user.role.upcase%> USER</h3>
    </div>
    
    <div class='profile_right_menu'>
      <h2><%= @user.name %></h2>
      <div class='about_me'><%= @user.about_me%>
      </div>
    </div>

  </div>

  <div class ='profile_left_list'>
    <ul>
      <tr>
      <td>Age</td>
      <td><%= @user.age %></td>
      <tr>
      <td>Gender</td>
      <td><%= @user.gender %></td>
      <tr>
      <td>City</td>
      <td><%= @user.city %></td>
      <tr>
      <td>Email</td>
      <td><%= @user.email %></td>
      <tr>
      <td>Interested in</td>
      <td><%= @user.sex_preference %></td>
      <tr>
      <td>Height in cm</td>
      <td><%= @user.height %></td>
      <tr>
      <td>Smoker</td>
      <td><%= @user.smoker %></td>
      <tr>
      <td>GitHub ID</td>
      <td><%= @user.github_id %></td>
      <tr>
      <td>Favourite code language</td>
      <td><%= @user.favourite_language %></td>
      <tr>
      <td>Stack Overflow Score</td>
      <td><%= @user.stackoverflow_score %></td>
      <tr>
      <td>Meet Ups</td>
      <td><%= @user.meet_ups %></td>
      <tr>
      <td>Front-end Or Back-end</td>
      <td><%= @user.front_backend  %></td>
    </ul>
  </div>
  <div class ='profile_right_list'>
    <ul>

    </ul>
  </div>
<br>

  <%= link_to "#{@user.impressionist_count(start_date:Time.now-(24*60*60),end_date: Time.now)} visits to my profile in the past 3 days", user_receives_visit_path %>




